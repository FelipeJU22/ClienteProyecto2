<VBox alignment="CENTER" prefHeight="545.0" prefWidth="552.0" spacing="20.0" style="-fx-background-color: #788199;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientMain.MainController">
